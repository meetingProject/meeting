<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.projectmeeting" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/makemetting"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".onami_signup_cellphone"/>
        <activity android:name=".onami_signup_univ"/>
        <activity android:name=".onami_signup_id"/>
        <activity android:name=".onami_find_id"/>
        <activity android:name=".onami_find_pw"/>
        <activity android:name=".onami_find_pwchange"/>
        <activity android:name=".onami_signup_picture"/>
        <activity android:name=".onami_makeroom"/>
        <activity android:name=".onami_selectoption"/>
        <activity android:name=".onami_tabview"
            android:theme="@android:style/Theme.NoTitleBar"/>
        <activity android:name=".onami_maderoom"/>
        <activity android:name=".onami_setting"/>
        <activity android:name=".TabMeetingRoomActivity"/>
        <activity android:name=".onami_chat_list"/>
        <activity android:name=".onami_joinroom"/>

    </application>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.RECEIVE_SMS" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
</manifest>
